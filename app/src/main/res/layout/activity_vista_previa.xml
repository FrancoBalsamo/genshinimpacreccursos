<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Actividades.VistaPrevia"
    android:background="@color/black"
    android:orientation="horizontal">
    <LinearLayout
        android:id="@+id/creado"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="15dp"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginRight="15dp"
                    android:layout_marginLeft="15dp">
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_gravity="center">
                        <ImageView
                            android:layout_width="50dp"
                            android:layout_height="50dp"
                            android:background="@drawable/florico"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">
                        <TextView
                            android:id="@+id/tvF"
                            android:fontFamily="@font/gifontdos"
                            android:textColor="@color/white"
                            android:layout_width="160dp"
                            android:layout_height="20dp"
                            android:text="Stat Principal"
                            android:paddingLeft="15dp"/>
                        <TextView
                            android:id="@+id/tvFl"
                            android:fontFamily="@font/gifontuno"
                            android:textColor="@color/white"
                            android:layout_width="160dp"
                            android:layout_height="20dp"
                            android:text="Stat Secundario A"
                            android:paddingLeft="15dp"/>
                        <TextView
                            android:id="@+id/tvFlo"
                            android:fontFamily="@font/gifontuno"
                            android:textColor="@color/white"
                            android:layout_width="160dp"
                            android:layout_height="20dp"
                            android:text="Stat Secundario B"
                            android:paddingLeft="15dp"/>
                        <TextView
                            android:id="@+id/tvFlor"
                            android:fontFamily="@font/gifontuno"
                            android:textColor="@color/white"
                            android:layout_width="160dp"
                            android:layout_height="20dp"
                            android:text="Stat Secundario C"
                            android:paddingLeft="15dp"/>
                        <TextView
                            android:id="@+id/tvFlore"
                            android:fontFamily="@font/gifontuno"
                            android:textColor="@color/white"
                            android:layout_width="160dp"
                            android:layout_height="20dp"
                            android:text="Stat Secundario D"
                            android:paddingLeft="15dp"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_marginRight="15dp"
                    android:layout_marginLeft="15dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_gravity="center">
                        <ImageView
                            android:layout_width="50dp"
                            android:layout_height="50dp"
                            android:background="@drawable/plumico"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">
                        <TextView
                            android:id="@+id/P"
                            android:fontFamily="@font/gifontdos"
                            android:textColor="@color/white"
                            android:layout_width="160dp"
                            android:layout_height="20dp"
                            android:text="Stat Principal"
                            android:paddingLeft="15dp"/>
                        <TextView
                            android:id="@+id/Pl"
                            android:fontFamily="@font/gifontuno"
                            android:textColor="@color/white"
                            android:layout_width="160dp"
                            android:layout_height="20dp"
                            android:text="Stat Secundario A"
                            android:paddingLeft="15dp"/>
                        <TextView
                            android:id="@+id/Plu"
                            android:fontFamily="@font/gifontuno"
                            android:textColor="@color/white"
                            android:layout_width="160dp"
                            android:layout_height="20dp"
                            android:text="Stat Secundario B"
                            android:paddingLeft="15dp"/>
                        <TextView
                            android:id="@+id/Plum"
                            android:fontFamily="@font/gifontuno"
                            android:textColor="@color/white"
                            android:layout_width="160dp"
                            android:layout_height="20dp"
                            android:text="Stat Secundario C"
                            android:paddingLeft="15dp"/>
                        <TextView
                            android:id="@+id/Pluma"
                            android:fontFamily="@font/gifontuno"
                            android:textColor="@color/white"
                            android:layout_width="160dp"
                            android:layout_height="20dp"
                            android:text="Stat Secundario D"
                            android:paddingLeft="15dp"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_marginLeft="15dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_gravity="center">
                        <ImageView
                            android:layout_width="50dp"
                            android:layout_height="50dp"
                            android:background="@drawable/relico"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">
                        <TextView
                            android:id="@+id/R"
                            android:fontFamily="@font/gifontdos"
                            android:textColor="@color/white"
                            android:layout_width="160dp"
                            android:layout_height="20dp"
                            android:text="Stat Principal"
                            android:paddingLeft="15dp"/>
                        <TextView
                            android:id="@+id/Re"
                            android:fontFamily="@font/gifontuno"
                            android:textColor="@color/white"
                            android:layout_width="160dp"
                            android:layout_height="20dp"
                            android:text="Stat Secundario A"
                            android:paddingLeft="15dp"/>
                        <TextView
                            android:id="@+id/Rel"
                            android:fontFamily="@font/gifontuno"
                            android:textColor="@color/white"
                            android:layout_width="160dp"
                            android:layout_height="20dp"
                            android:text="Stat Secundario B"
                            android:paddingLeft="15dp"/>
                        <TextView
                            android:id="@+id/Relo"
                            android:fontFamily="@font/gifontuno"
                            android:textColor="@color/white"
                            android:layout_width="160dp"
                            android:layout_height="20dp"
                            android:text="Stat Secundario C"
                            android:paddingLeft="15dp"/>
                        <TextView
                            android:id="@+id/Reloj"
                            android:fontFamily="@font/gifontuno"
                            android:textColor="@color/white"
                            android:layout_width="160dp"
                            android:layout_height="20dp"
                            android:text="Stat Secundario D"
                            android:paddingLeft="15dp"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:layout_marginTop="15dp"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp">
                <LinearLayout
                    android:layout_marginTop="40dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginLeft="15dp">
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_gravity="center">
                        <ImageView
                            android:layout_width="50dp"
                            android:layout_height="50dp"
                            android:background="@drawable/copico"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">
                        <TextView
                            android:id="@+id/C"
                            android:fontFamily="@font/gifontdos"
                            android:textColor="@color/white"
                            android:layout_width="160dp"
                            android:layout_height="20dp"
                            android:text="Stat Principal"
                            android:paddingLeft="15dp"/>
                        <TextView
                            android:id="@+id/Co"
                            android:fontFamily="@font/gifontuno"
                            android:textColor="@color/white"
                            android:layout_width="160dp"
                            android:layout_height="20dp"
                            android:text="Stat Secundario A"
                            android:paddingLeft="15dp"/>
                        <TextView
                            android:id="@+id/Cop"
                            android:fontFamily="@font/gifontuno"
                            android:textColor="@color/white"
                            android:layout_width="160dp"
                            android:layout_height="20dp"
                            android:text="Stat Secundario B"
                            android:paddingLeft="15dp"/>
                        <TextView
                            android:id="@+id/Copa"
                            android:fontFamily="@font/gifontuno"
                            android:textColor="@color/white"
                            android:layout_width="160dp"
                            android:layout_height="20dp"
                            android:text="Stat Secundario C"
                            android:paddingLeft="15dp"/>
                        <TextView
                            android:id="@+id/Copas"
                            android:fontFamily="@font/gifontuno"
                            android:textColor="@color/white"
                            android:layout_width="160dp"
                            android:layout_height="20dp"
                            android:text="Stat Secundario D"
                            android:paddingLeft="15dp"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="270dp"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">
                    <ImageView
                        android:id="@+id/bkgPJ"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_marginTop="40dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_gravity="center">
                        <ImageView
                            android:layout_width="50dp"
                            android:layout_height="50dp"
                            android:background="@drawable/corico"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">
                        <TextView
                            android:id="@+id/tvC"
                            android:fontFamily="@font/gifontdos"
                            android:textColor="@color/white"
                            android:layout_width="160dp"
                            android:layout_height="20dp"
                            android:text="Stat Principal"
                            android:paddingLeft="15dp"/>
                        <TextView
                            android:id="@+id/tvCo"
                            android:fontFamily="@font/gifontuno"
                            android:textColor="@color/white"
                            android:layout_width="160dp"
                            android:layout_height="20dp"
                            android:text="Stat Secundario A"
                            android:paddingLeft="15dp"/>
                        <TextView
                            android:id="@+id/tvCor"
                            android:fontFamily="@font/gifontuno"
                            android:textColor="@color/white"
                            android:layout_width="160dp"
                            android:layout_height="20dp"
                            android:text="Stat Secundario B"
                            android:paddingLeft="15dp"/>
                        <TextView
                            android:id="@+id/tvCoro"
                            android:fontFamily="@font/gifontuno"
                            android:textColor="@color/white"
                            android:layout_width="160dp"
                            android:layout_height="20dp"
                            android:text="Stat Secundario C"
                            android:paddingLeft="15dp"/>
                        <TextView
                            android:id="@+id/tvCoron"
                            android:fontFamily="@font/gifontuno"
                            android:textColor="@color/white"
                            android:layout_width="160dp"
                            android:layout_height="20dp"
                            android:text="Stat Secundario D"
                            android:paddingLeft="15dp"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</RelativeLayout>